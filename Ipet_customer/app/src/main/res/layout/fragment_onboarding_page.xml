<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.onboarding.OnBoardingPageFragment">

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/lottieAnimationView_onboardingpage_first"
        android:layout_width="96dp"
        android:layout_height="68dp"
        android:layout_marginTop="60dp"
        app:layout_constraintEnd_toEndOf="@id/guideLine_oboarding_end_boundary"
        app:layout_constraintTop_toTopOf="@id/textView_onboardingpage_first_descript1"
        app:lottie_loop="true" />

    <TextView
        android:id="@+id/textView_onboardingpage_first_descript1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAlignment="center"
        android:fontFamily="@font/yeongdo"
        android:textSize="96sp"
        android:textColor="@color/brown_200"
        app:layout_constraintBottom_toTopOf="@+id/textView_onboardingpage_third_descript1"
        app:layout_constraintEnd_toEndOf="@id/guideLine_oboarding_end_boundary"
        app:layout_constraintStart_toStartOf="@id/guideLine_oboarding_start_boundary"
        app:layout_constraintTop_toBottomOf="@id/lottieAnimationView_onboardingpage_second" />

    <TextView
        android:id="@+id/textView_onboardingpage_first_descript2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="@style/Typography.Bold24"
        android:textColor="@color/rose_100"
        android:textAlignment="center"
        android:layout_marginBottom="84dp"
        app:layout_constraintBottom_toBottomOf="@id/textView_onboardingpage_first_descript1"
        app:layout_constraintStart_toStartOf="@id/guideLine_oboarding_start_boundary"
        app:layout_constraintEnd_toEndOf="@id/guideLine_oboarding_end_boundary"/>

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/lottieAnimationView_onboardingpage_second"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="@id/guideLine_oboarding_end_boundary"
        app:layout_constraintStart_toStartOf="@id/guideLine_oboarding_start_boundary"
        app:layout_constraintTop_toTopOf="@+id/guideLine_oboarding_lottie_boundary"
        app:lottie_loop="true" />

    <TextView
        android:id="@+id/textView_onboardingpage_second_descript1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:textAppearance="@style/Typography.Bold24"
        android:textColor="@color/rose_100"
        app:layout_constraintTop_toBottomOf="@id/lottieAnimationView_onboardingpage_second"
        app:layout_constraintStart_toStartOf="@id/guideLine_oboarding_start_boundary"/>

    <TextView
        android:id="@+id/textView_onboardingpage_second_descript2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textAppearance="@style/Typography.Bold32"
        android:textColor="@color/brown_200"
        app:layout_constraintTop_toBottomOf="@id/textView_onboardingpage_second_descript1"
        app:layout_constraintStart_toStartOf="@id/guideLine_oboarding_start_boundary"
        app:layout_constraintEnd_toEndOf="@id/guideLine_oboarding_end_boundary"/>

    <TextView
        android:id="@+id/textView_onboardingpage_second_descript3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textAppearance="@style/Typography.Bold32"
        android:textColor="@color/brown_200"
        app:layout_constraintTop_toBottomOf="@id/textView_onboardingpage_second_descript2"
        app:layout_constraintStart_toStartOf="@id/guideLine_oboarding_start_boundary"
        app:layout_constraintEnd_toEndOf="@id/guideLine_oboarding_end_boundary"/>

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/lottieAnimationView_onboardingpage_third_cat"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="10dp"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/guideLine_oboarding_cat_boundary"
        app:lottie_loop="true" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/lottieAnimationView_onboardingpage_third_dog"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintEnd_toEndOf="@id/guideLine_oboarding_end_boundary"
        app:layout_constraintBottom_toTopOf="@+id/guideLine_oboarding_dog_boundary"
        app:lottie_loop="true" />

    <TextView
        android:id="@+id/textView_onboardingpage_third_descript1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:textAppearance="@style/Typography.Bold20"
        android:textColor="@color/rose_100"
        app:layout_constraintTop_toBottomOf="@id/lottieAnimationView_onboardingpage_second"
        app:layout_constraintStart_toStartOf="@id/guideLine_oboarding_start_boundary"
        app:layout_constraintEnd_toEndOf="@id/guideLine_oboarding_end_boundary"/>

    <TextView
        android:id="@+id/textView_onboardingpage_third_descript2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textAppearance="@style/Typography.Bold32"
        android:textColor="@color/brown_200"
        app:layout_constraintTop_toBottomOf="@id/textView_onboardingpage_second_descript1"
        app:layout_constraintStart_toStartOf="@id/guideLine_oboarding_start_boundary"
        app:layout_constraintEnd_toEndOf="@id/guideLine_oboarding_end_boundary"/>

    <TextView
        android:id="@+id/textView_onboardingpage_third_descript3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textAppearance="@style/Typography.Bold32"
        android:textAlignment="textEnd"
        android:textColor="@color/brown_200"
        app:layout_constraintTop_toBottomOf="@id/textView_onboardingpage_second_descript2"
        app:layout_constraintStart_toStartOf="@id/guideLine_oboarding_start_boundary"
        app:layout_constraintEnd_toEndOf="@id/guideLine_oboarding_end_boundary"/>

    <TextView
        android:id="@+id/textView_onboardingpage_third_descript4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textAppearance="@style/Typography.Bold20"
        android:textAlignment="textEnd"
        android:textColor="@color/rose_100"
        app:layout_constraintTop_toBottomOf="@id/textView_onboardingpage_second_descript3"
        app:layout_constraintStart_toStartOf="@id/guideLine_oboarding_start_boundary"
        app:layout_constraintEnd_toEndOf="@id/guideLine_oboarding_end_boundary"/>

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/lottieAnimationView_onboardingpage_fourth"
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:layout_marginTop="30dp"
        app:layout_constraintEnd_toEndOf="@id/guideLine_oboarding_end_boundary"
        app:layout_constraintStart_toStartOf="@id/guideLine_oboarding_start_boundary"
        app:layout_constraintTop_toTopOf="@+id/guideLine_oboarding_lottie_boundary"
        app:lottie_loop="true" />

    <TextView
        android:id="@+id/textView_onboardingpage_fourth_descript1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textAppearance="@style/Typography.Bold24"
        android:textAlignment="center"
        android:layout_marginTop="20dp"
        android:textColor="@color/rose_100"
        app:layout_constraintTop_toBottomOf="@id/lottieAnimationView_onboardingpage_fourth"
        app:layout_constraintStart_toStartOf="@id/guideLine_oboarding_start_boundary"
        app:layout_constraintEnd_toEndOf="@id/guideLine_oboarding_end_boundary"/>

    <TextView
        android:id="@+id/textView_onboardingpage_fourth_descript2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textAppearance="@style/Typography.Bold32"
        android:textAlignment="center"
        android:textColor="@color/brown_200"
        app:layout_constraintTop_toBottomOf="@id/textView_onboardingpage_fourth_descript1"
        app:layout_constraintStart_toStartOf="@id/guideLine_oboarding_start_boundary"
        app:layout_constraintEnd_toEndOf="@id/guideLine_oboarding_end_boundary"/>


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideLine_oboarding_lottie_boundary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="20dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideLine_oboarding_cat_boundary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="300dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideLine_oboarding_dog_boundary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="330dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideLine_oboarding_start_boundary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="50dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideLine_oboarding_end_boundary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_end="50dp" />
</androidx.constraintlayout.widget.ConstraintLayout>